{
  "rules": {
    "indent": [2, 2],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-undef": 2
  },
  "globals": {
    "$": true,
    "module": true,
    "define": true,
    "jQuery": true,
    "require": true
  },
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended"
}
